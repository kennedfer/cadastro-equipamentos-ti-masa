generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Dispositivo {
  id        Int         @id @default(autoincrement()) 
  tipo      String?                 
  marca     String?                   
  modelo    String?                   
  tag       String      @unique     
  serial    String      @unique     
  cautela   String?             
  status    String?                                 
//  atributos Atributo?                             
}

