generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider  = "postgresql"
  url       = env("DATABASE_URL")
  directUrl = env("DIRECT_URL")
}

model Dispositivo {
  id         Int    @id @default(autoincrement())
  cpf        String
  funcao     String
  marca      String
  modelo     String
  nome       String
  patrimonio String @unique
  setor      String
  status     String
  tag        String @unique
  tipo       String
}

model Usuario {
  id         BigInt   @id @default(autoincrement())
  created_at DateTime @default(now()) @db.Timestamptz(6)
  apelido    String?
  funcao     Funcoes?
  senha      String?
}

enum Funcoes {
  SUPER
  ADMIN
}
